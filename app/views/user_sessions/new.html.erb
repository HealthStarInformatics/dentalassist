
<% form_for (@user_session, :url => { :action => "create" }) do |f|%>
  <%= f.error_messages %>
  <div class="row">
		<div class="text-field">
			<label>Username</label>
		</div>
		<div class="text-field">
			<%= f.text_field :username %>
		</div>
  </div>
  <div class="row">
		<div class="text-field">
			<%= f.label :password %><br />
		</div>
		<div class="text-field">
			<%= f.password_field :password %>
		</div>
  </div>
  <div class="row"
		  <%= f.submit "Submit" %></p>
  </div>
<% end %>
