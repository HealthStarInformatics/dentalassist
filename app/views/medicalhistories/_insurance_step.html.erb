			<%= f.input :secondary_group, :label=>"Group/Policy",:label_html => {:class => "form_label"}%>	
			<%= f.input :secondary_insured_name, :label => "Insured's Name",:label_html => {:class => "form_label"}%>	
			<%= f.input :secondary_insured_dob, :label => "Insured's Birth date", :as=>:string, :input_html=>{:size=>7, :id => 'date1', :class=>"some"},:label_html => {:class => "form_label"}%>	
			<%= f.input :secondary_relation, :label => "Relation",:label_html => {:class => "form_label"}%>	
			<%= f.input :secondary_ssn, :label => "Insured's SS#",:label_html => {:class => "form_label"}%>	
			<%= f.input :secondary_employer, :label => "Insured's Employer",:label_html => {:class => "form_label"}%>	
	</fieldset>
