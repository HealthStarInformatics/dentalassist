<div id="C_header">
		<div id="C_headerBody">
				<div id="C_userNav">
						<ul id="C_userLinks">
								<li class="C_userNavItem">
                <%if normal_user? %>
                  <%= link_to "Edit Form", edit_medicalhistory_path(current_user.id)%> |
                  <%= link_to "Edit Profile", edit_user_path(:current)%> |
                  <a class="J_loginLink J_onClick headerLogin" href="/logout/">Logout</a>
                <%elsif location_admin?%>
                  <%= link_to "Edit Profile", edit_user_path(:current)%> |
                  <a class="J_loginLink J_onClick headerLogin" href="/logout/">Logout</a>
                <%elsif super_admin?%>
                  <%= link_to "Add Dentistry", new_dentistry_path%> |
                  <%= link_to "Add Location", new_location_path%> |
                  <%= link_to "Edit Profile", edit_user_path(:current)%> |
                  <a class="J_loginLink J_onClick headerLogin" href="/logout/">Logout</a>
                <%else%>
                  <a class="J_loginLink J_onClick headerLogin" href="/login/">Log in</a>
                <%end%>
                </li>
						</ul>
				</div>
				<div id="C_globalNav">
						<div id="C_tabs">
                <% if location_admin? or super_admin? %>
								<a href="/medicalhistories/">
										<span class="C_tabContent">
										  <span class="C_topBig">Patient Forms</span>
										  <span class="C_topSm">All patients in locaiton</span>
										</span>
								</a>
                <% elsif current_user %>
								<a href="/medicalhistories/">
										<span class="C_tabContent">
										  <span class="C_topBig">Patient Dashboard</span>
										  <span class="C_topSm">Change your preferances</span>
										</span>
								</a>
                <% else %> 
								<a href="/medicalhistories/new">
										<span class="C_tabContent">
										  <span class="C_topBig">New Patients</span>
										  <span class="C_topSm">Fill Forms</span>
										</span>
								</a>
                <% end %>
								<a href="/appointment" class="omnCamp omnic_sn1" id="tabs_start">
										<span class="C_startContainer C_tabContent">
										  <span class="C_topBig">Appointments</span>
										  <span class="C_topSm"><%if location_admin?%>view upcoming appointments<%else%>view scheduled appointments<%end%></span>
										</span>
								</a>
								<a href="/locations" class="omnCamp omnrg_perksheader" >
										<span class="C_startContainer C_tabContent">
										  <span class="C_topBig">Locations</span>
										  <span class="C_topSm"><%if location_admin?%>manage locations<%else%>list locations<%end%></span>
										</span>
								</a>
						</div>
				</div>
		</div>
</div>

<div id="C_pageBody">
    <div id="container">
      <% flash.each do |name, msg| %>
        <%= content_tag :div, msg, :id => "flash_#{name}" %>
      <% end %>
      <%= content_tag :h1, yield(:title) if show_title? %>
      <%= yield %>
    </div>
</div>


  </body>
</html>














